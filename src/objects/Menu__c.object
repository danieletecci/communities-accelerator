    <validationRules>
        <fullName>LevelValidation</fullName>
        <active>true</active>
        <description>Gets an error if you add more than three related submenus</description>
        <errorConditionFormula>NOT(
OR(
ISBLANK(ParentMenu__c),
ISBLANK(ParentMenu__r.ParentMenu__c),
ISBLANK(ParentMenu__r.ParentMenu__r.ParentMenu__c)
)
)</errorConditionFormula>
        <errorMessage>Sorry, this menu already has the max of submenu levels, you can&apos;t add more.</errorMessage>
    </validationRules>
