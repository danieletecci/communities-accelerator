    <validationRules>
        <fullName>NoDefaultValue</fullName>
        <active>true</active>
        <description>This Validation Rule prevents the User to save a Default value when the field type for the Form Element is equal to &quot;Date&quot;.</description>
        <errorConditionFormula>ISPICKVAL(FieldType__c, &apos;Date&apos;) &amp;&amp; NOT(ISBLANK( DefaultValue__c))</errorConditionFormula>
        <errorDisplayField>DefaultValue__c</errorDisplayField>
        <errorMessage>Can&apos;t set Default Value for Field Type = Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TextAreaRowsOnlyForTextArea</fullName>
        <active>true</active>
        <errorConditionFormula>(  ISPICKVAL(FieldType__c,&apos;TextArea&apos;) &amp;&amp; ( ISBLANK(TextAreaRows__c) || TextAreaRows__c &lt; 1) ) || (NOT(ISPICKVAL(FieldType__c,&apos;TextArea&apos;)) &amp;&amp; NOT(ISBLANK(TextAreaRows__c)))</errorConditionFormula>
        <errorMessage>TextAreaRows can only be set for TextArea type fields.</errorMessage>
    </validationRules>
