<aura:component extends="c:CG_BaseComponent" implements="forceCommunity:availableForAllPageTypes">
	
	<!-- GENERAL VARIABLES --> 
	<aura:attribute name="componentExternalId" type="String" />
	<aura:attribute name="componentType" type="String" default="AdvanceContent"/>

	<!-- ADVANCE CONTENT COMPONENT -->
	<aura:iteration items="{!v.componentWrapper.contentWrapper}" var="wrapper">
		<div>
			<h1>{!wrapper.content.DetailTitle__c}</h1>
			<aura:unescapedHtml value="{!wrapper.content.HTMLContent__c}"/>
		</div>
	</aura:iteration>
                    
</aura:component>